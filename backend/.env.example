# 数据库配置
DB_HOST=your-db-host
DB_PORT=5432
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_NAME=your-db-name
DB_SSLMODE=disable

# 服务器配置
PORT=8080
ENV=development

# CORS 配置（逗号分隔）
ALLOWED_ORIGINS=http://localhost:3000
ALLOW_CREDENTIALS=false

# 限流与请求体限制
RATE_LIMIT_PER_MINUTE=60
MAX_BODY_BYTES=1048576